#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int a,b,c,d,k;
        cin>>a>>b>>c>>d>>k;
        int y =abs(c-a) + abs(d-b);
        string s= (y <= k && (k-y)%2==0)?"yes":"no";
        cout<<s<<"\n";
    }
    return 0;
}
